# ============================================
# Frontend Environment Variables
# ============================================

# API URL - Change this to your Railway backend URL after deployment
# Example: VITE_API_URL=https://scck-api.up.railway.app
VITE_API_URL=http://localhost:3000

# App Name
VITE_APP_NAME=SCCK ERP NEXUS

# App Version
VITE_APP_VERSION=1.0.0

# Default Currency
VITE_CURRENCY=CFA

# Enable/Disable Demo Mode
VITE_ENABLE_DEMO=true
