{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".vercel/output",
  "framework": "express",
  "env": {
    "NODE_ENV": "production"
  },
  "envPrefix": "",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "^/api/.*",
      "dest": "/api/index.js"
    },
    {
      "src": "^/assets/(.*)$",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(?!api|_next|assets|.*\\..*)",
      "dest": "/src/index.html"
    }
  ]
}